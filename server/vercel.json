{
  "version": 2,
  "builds": [
    {
      "src": "Server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "models/**",
          "controllers/**",
          "routes/**",
          "configs/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Server.js"
    }
  ]
}